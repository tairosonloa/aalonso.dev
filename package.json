{
  "name": "aalonso.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/prebuild.js",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc",
    "g:component": "hygen component new",
    "g:production-build": "npm run prebuild && next build && next export",
    "lint:ts": "eslint --fix \"**/*.{ts,tsx}\" --no-error-on-unmatched-pattern",
    "lint:css": "stylelint --fix \"**/*.scss\"",
    "lint:css:ci": "stylelint --max-warnings 0 \"**/*.scss\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,scss}\" \"*.{js,json}\"",
    "precommit": "npm run typecheck && npm run format && npm run lint:ts && npm run lint:css && npm run test"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "devicon": "2.2.0",
    "lodash": "4.17.15",
    "next": "9.4.4",
    "next-seo": "4.7.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remixicon": "2.5.0",
    "sanitize-html": "1.27.0",
    "sass": "1.26.10",
    "sitemap": "6.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-backgrounds": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.4",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.4",
    "@types/lodash": "4.14.157",
    "@types/next": "9.0.0",
    "@types/node": "14.0.19",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/sanitize-html": "1.23.3",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "eslint": "7.4.0",
    "eslint-config-airbnb-typescript-prettier": "3.1.0",
    "hygen": "5.0.3",
    "jest": "26.1.0",
    "netlify-cli": "2.58.0",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-property-sort-order-smacss": "6.3.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "typescript": "3.9.6"
  },
  "author": "Aitor Alonso (@tairosonloa)",
  "jest": {
    "transform": {
      "^.+\\.module.scss$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  }
}
