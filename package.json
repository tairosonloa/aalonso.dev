{
  "name": "next-boilerplate-typescript-jest",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test:coverage": "yarn test --coverage",
    "test:debug": "yarn test --debug --detectOpenHandles",
    "test:ci": "yarn test --runInBand --no-cache --ci",
    "test": "NODE_ENV=test jest -w=75%",
    "lint": "eslint './{src,pages}/**/*' --ext .js,.jsx,.tsx,.ts",
    "format": "eslint './{src,pages}/**/*' --ext .js,.jsx,.tsx,.ts --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "export": "cross-env NODE_ENV=production next export",
    "production": "npm-run-all analyze export",
    "production:clean": "rimraf out/"
  },
  "dependencies": {
    "@headlessui/react": "^0.3.1",
    "@tailwindcss/typography": "^0.4.0",
    "date-fns": "^2.19.0",
    "next": "^10.0.7",
    "next-google-fonts": "^2.1.0",
    "next-seo": "^4.23.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@types/node": "^14.14.36",
    "@types/prismjs": "^1.16.3",
    "@types/react": "^17.0.2",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  }
}
