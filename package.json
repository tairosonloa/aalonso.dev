{
  "name": "aalonso.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc",
    "g:component": "hygen component new",
    "lint:ts": "eslint --fix \"**/*.{ts,tsx}\" --no-error-on-unmatched-pattern",
    "lint:css": "stylelint --fix \"**/*.scss\"",
    "lint:css:ci": "stylelint --max-warnings 0 \"**/*.scss\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,scss}\" \"*.{js,json}\"",
    "precommit": "npm run typecheck && npm run format && npm run lint:ts && npm run lint:css && npm run test"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remixicon": "^2.5.0",
    "sass": "^1.26.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "@types/next": "^9.0.0",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "hygen": "^5.0.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-property-sort-order-smacss": "^6.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.9.3"
  },
  "author": "Aitor Alonso (@tairosonloa)",
  "jest": {
    "transform": {
      "^.+\\.module.scss$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  }
}
